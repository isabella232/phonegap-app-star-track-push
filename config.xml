<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.startrack" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Star Track</name>
    <description>
        A basic Framework7 Media App for PhoneGap.
    </description>
    <author email="tommy@devgeeks.org" href="http://phonegap.com">
        Adobe PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <preference name="StatusBarStyle" value="default" />
    <plugin name="cordova-plugin-media" spec="~2.3.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.1.0" />
    <plugin name="phonegap-plugin-push" spec="~1.10.4" />
    <preference name="FadeSplashScreenDuration" value="300" />
</widget>
